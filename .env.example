APP_NAME="UserDetect API"
APP_ENV=local
# REQUIRED: Generate with: php artisan key:generate
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=userdetect
# REQUIRED: Set database credentials
DB_USERNAME=postgres
DB_PASSWORD=
DB_SSLMODE=prefer

BROADCAST_CONNECTION=log
CACHE_STORE=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=false

REDIS_HOST=127.0.0.1
# REQUIRED for production: Set a strong Redis password
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log

API_RATE_LIMIT_PER_MINUTE=100
API_TIMEOUT_SECONDS=10
ENSEMBLE_TIMEOUT_SECONDS=3
ENSEMBLE_CONNECT_TIMEOUT_SECONDS=2
ENSEMBLE_MIN_SOURCES_REQUIRED=2
ENSEMBLE_FAILURE_CIRCUIT_THRESHOLD=4
ENSEMBLE_FAILURE_CIRCUIT_TTL_SECONDS=120
ENSEMBLE_ALLOW_INSECURE_SOURCES=false
ENSEMBLE_ENABLED_SOURCES=ipapi,ipwhois,ipwho,freeipapi
DETECTION_RETENTION_DAYS=90
FINGERPRINT_RETENTION_DAYS=90
LEARNING_REQUIRE_VERIFIED_LOCATION=true
DETECTION_DEFAULT_COUNTRY=India
DETECT_SLO_P95_MS=700

IPAPI_KEY=
IPGEOLOCATION_KEY=

# Use comma-separated origins for production, e.g.: https://example.com,https://www.example.com
CORS_ALLOWED_ORIGINS=*

# Test IP for local development (used when request comes from localhost/private IP)
TEST_IP=106.215.153.11

# REQUIRED: MaxMind GeoLite2 â€” Free license key from https://www.maxmind.com/en/geolite2/signup
MAXMIND_LICENSE_KEY=

# Trusted Proxies (Cloudflare)
TRUSTED_PROXIES=*
